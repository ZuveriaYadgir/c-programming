/*Aim: WAP ro add two numbers using pointers.*/
#include <stdio.h>
int main()
{ int num1, num2, sum;
int *ptr1 , *ptr2;
printf("Enter first number:");
scanf("%d" , & num1);
printf("Enter second number: ");
scanf("%d" , & num2);
ptr1= &num1;
ptr2= &num2;
sum = *ptr1 + *ptr2;
printf("Sum of %d and %d is : %d\n" , num1 , num2 , sum);
return 0;}
/*
Enter first number:12
Enter second number: 13
Sum of 12 and 13 is : 25
*/
