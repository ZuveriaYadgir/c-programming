#include<stdio.h>
int main()
{
    char str[100];
    int length=0;   //variable
    printf("\t\t*** String Length Finder ***\n\n\n");
    printf("Enter Your String : ");
    gets(str);
    while(str[length] !='\0')
    {length++;}

    printf("string length: %d Characters",length);
    return 0;}
/*

*** String Length Finder ***
Enter Your String : hello world
string length: 11 Characters
*/
